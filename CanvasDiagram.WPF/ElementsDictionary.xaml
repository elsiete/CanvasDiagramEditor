<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:CanvasDiagram.WPF.Controls;assembly=CanvasDiagram.WPF.Controls"
                    xmlns:elements="clr-namespace:CanvasDiagram.WPF.Elements"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/CanvasDiagram.WPF.Resources;component/BaseDictionary.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <ControlTemplate x:Key="PinControlTemplateKey">
        <elements:PinControl/>
    </ControlTemplate>

    <ControlTemplate x:Key="InputControlTemplateKey">
        <elements:InputControl/>
    </ControlTemplate>

    <ControlTemplate x:Key="OutputControlTemplateKey">
        <elements:OutputControl/>
    </ControlTemplate>

    <ControlTemplate x:Key="AndGateControlTemplateKey">
        <elements:AndGateControl/>
    </ControlTemplate>

    <ControlTemplate x:Key="OrGateControlTemplateKey">
        <elements:OrGateControl/>
    </ControlTemplate>

    <ControlTemplate x:Key="LandscapePageTemplateKey">
        <elements:PageControl/>
    </ControlTemplate>
    
</ResourceDictionary>